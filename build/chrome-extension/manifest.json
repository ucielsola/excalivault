{
  "manifest_version": 3,
  "name": "Excalivault",
  "version": "1.0.0",
  "description": "A Chrome extension vault for your Excalidraw drawings",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Excalivault"
  },
  "icons": {}
}